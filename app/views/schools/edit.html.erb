<%= form_tag("/schools/#{@school.id}", method: :put) do %>
  <%= label_tag "uid" %>
  <%= text_field_tag "school[uid]", @school.uid %>
  <%= label_tag "overweight_percentage" %>
  <%= text_field_tag "school[overweight_percentage]", @school.overweight_percentage %>
  <%= label_tag "obese_percentage" %>
  <%= text_field_tag "school[obese_percentage]", @school.obese_percentage %>
  <%= submit_tag "Create" %>
<% end %>
